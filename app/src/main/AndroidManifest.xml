            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.naman.nutritionchoice.activity.RajasthanListActivity" />
        </activity>
        <activity
            android:name=".activity.TamilNaduListActivity"
            android:label="@string/title_tamilnadu_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.TamilNaduDetailActivity"
            android:label="@string/title_tamilnadu_detail"
            android:parentActivityName=".activity.TamilNaduListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.naman.nutritionchoice.activity.TamilNaduListActivity" />
        </activity>
        <activity
            android:name=".activity.MaharashtraListActivity"
            android:label="@string/title_maharashtra_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.MaharashtraDetailActivity"
            android:label="@string/title_maharashtra_detail"
            android:parentActivityName=".activity.MaharashtraListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.naman.nutritionchoice.activity.MaharashtraListActivity" />
        </activity>
    </application>

</manifest>